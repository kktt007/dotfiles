;; [dotfiles/.stumpwmrc at master · r0man/dotfiles](https://github.com/r0man/dotfiles/blob/master/stumpwm/.stumpwmrc)

(in-package :stumpwm)

(defcommand rofi () ()
  "Start rofi"
  (run-shell-command "rofi -show run -combi-modi run,window"))
(define-key *top-map* (kbd "M-SPC") "rofi")
